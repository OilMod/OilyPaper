+package org.oilmod.bukkit.enchantments;
+
+import net.minecraft.server.EnchantmentSlotType;
+import net.minecraft.server.EnchantmentSlotType.OilSpecialEnchantmentSlotType;
+import net.minecraft.server.EnchantmentSlotType.VanillaEnchantmentSlotType;
+import net.minecraft.server.Item;
+import org.apache.commons.lang3.NotImplementedException;
+import org.apache.commons.lang3.Validate;
+import org.oilmod.api.OilMod;
+import org.oilmod.api.items.EnchantmentType;
+import org.oilmod.api.items.NMSItem;
+import org.oilmod.api.items.nms.NMSEnchantmentType;
+import org.oilmod.bukkit.OilMain;
+import org.oilmod.bukkit.block.RealNMSMaterial;
+
+import org.oilmod.api.items.EnchantmentType.EnchantmentTypeEnum;
+
+import java.security.AllPermission;
+import java.util.Set;
+
+public class RealEnchantmentTypeHelper extends EnchantmentType.EnchantmentTypeHelper {
+    private boolean initialised;
+
+
+    private SpecialEnchantmentType ALL;
+    private SpecialEnchantmentType ALL_VANILLA;
+    private RealEnchantmentType ARMOR;
+    private RealEnchantmentType ARMOR_BOOTS;
+    private RealEnchantmentType ARMOR_LEGGINGS;
+    private RealEnchantmentType ARMOR_CHEST;
+    private RealEnchantmentType ARMOR_HELMET;
+    private RealEnchantmentType WEAPON;
+    private RealEnchantmentType DIGGER;
+    private RealEnchantmentType FISHING_ROD;
+    private RealEnchantmentType BREAKABLE;
+    private RealEnchantmentType BOW;
+    private RealEnchantmentType WEARABLE;
+    private SpecialEnchantmentType NONE;
+
+    @Override
+    protected void apiInit() {
+        Validate.isTrue(!initialised, "Already initialised");
+        initialised = true;
+        NONE = new SpecialEnchantmentType(EnchantmentSlotType.NONE, OilMain.ModOilMod, EnchantmentTypeEnum.NONE) {
+            @Override
+            public boolean canEnchant(NMSItem item) {
+                return false;
+            }
+
+            @Override
+            public void addSubtype(EnchantmentType enchantmentType) {
+                throw new IllegalStateException("Cannot add to NONE");
+            }
+        };
+        BREAKABLE = new RealEnchantmentType(EnchantmentSlotType.BREAKABLE, EnchantmentTypeEnum.BREAKABLE);
+        BOW = new RealEnchantmentType(EnchantmentSlotType.BOW, EnchantmentTypeEnum.BOW);
+        WEARABLE = new RealEnchantmentType(EnchantmentSlotType.WEARABLE, EnchantmentTypeEnum.WEARABLE);
+        FISHING_ROD = new RealEnchantmentType(EnchantmentSlotType.FISHING_ROD, EnchantmentTypeEnum.FISHING_ROD);
+        DIGGER = new RealEnchantmentType(EnchantmentSlotType.DIGGER, EnchantmentTypeEnum.DIGGER);
+        WEAPON = new RealEnchantmentType(EnchantmentSlotType.WEAPON, EnchantmentTypeEnum.WEAPON);
+        ARMOR_BOOTS = new RealEnchantmentType(EnchantmentSlotType.ARMOR_FEET, EnchantmentTypeEnum.ARMOR_BOOTS);
+        ARMOR_LEGGINGS = new RealEnchantmentType(EnchantmentSlotType.ARMOR_LEGS, EnchantmentTypeEnum.ARMOR_LEGGINGS);
+        ARMOR_CHEST = new RealEnchantmentType(EnchantmentSlotType.ARMOR_CHEST, EnchantmentTypeEnum.ARMOR_CHEST);
+        ARMOR_HELMET = new RealEnchantmentType(EnchantmentSlotType.ARMOR_HEAD, EnchantmentTypeEnum.ARMOR_HELMET);
+        ARMOR = new RealEnchantmentType(EnchantmentSlotType.ARMOR, EnchantmentTypeEnum.ARMOR, ARMOR_BOOTS, ARMOR_LEGGINGS, ARMOR_CHEST, ARMOR_HELMET);
+        ALL_VANILLA = new SpecialEnchantmentType(EnchantmentSlotType.ALL_VANILLA, OilMain.ModMinecraft, EnchantmentTypeEnum.ALL_VANILLA, WEARABLE, BOW, BREAKABLE, FISHING_ROD, DIGGER, WEAPON, ARMOR_BOOTS, ARMOR_LEGGINGS, ARMOR_CHEST, ARMOR_HELMET, ARMOR) ;
+        ALL = new SpecialEnchantmentType(EnchantmentSlotType.ALL_INCL_CUSTOM, OilMain.ModOilMod, EnchantmentTypeEnum.ALL) {
+            @Override
+            public Set<EnchantmentType> getSubtypes() {
+                return EnchantmentType.getAll();
+            }
+
+            @Override
+            public boolean canEnchant(NMSItem item) {
+                for (EnchantmentType enchantmentType:EnchantmentType.getAll()) {
+                    if (enchantmentType.canEnchant(item)) return true;
+                }
+                return false;
+            }
+
+            @Override
+            public boolean containsSubtype(EnchantmentType enchantmentType) {
+                return true;
+            }
+
+            @Override
+            public boolean containsSubtypeDirectly(EnchantmentType enchantmentType) {
+                return false;
+            }
+
+            @Override
+            public void addSubtype(EnchantmentType enchantmentType) {
+                throw new IllegalStateException("Cannot add to ALL");
+            }
+
+            @Override
+            public void removeSubtype(EnchantmentType enchantmentType) {
+                throw new IllegalStateException("Cannot remove from ALL");
+            }
+        };
+    }
+
+    @Override
+    protected void apiPostInit() {
+
+    }
+
+    @Override
+    protected EnchantmentType getVanillaEnchantmentType(EnchantmentType.EnchantmentTypeEnum enchantType) {
+        EnchantmentType result = EnchantmentType.getStandard(enchantType);
+        Validate.notNull(result, "Misses implementation for vanilla enchantment type " + enchantType.toString());
+        return result;
+    }
+
+    @Override
+    protected NMSEnchantmentType registerCustom(EnchantmentType enchantType) {
+        return new EnchantmentSlotType.CustomEnchantmentSlotType(enchantType);
+    }
+
+    public static class RealEnchantmentType extends EnchantmentType {
+        private final VanillaEnchantmentSlotType nms;
+
+        protected RealEnchantmentType(NMSEnchantmentType nmsEnchantmentType, EnchantmentTypeEnum enchantmentTypeEnum, RealEnchantmentType... subtypes) {
+            super(nmsEnchantmentType, OilMain.ModMinecraft.createKey("enchant_type_" + nmsEnchantmentType.toString().toLowerCase()), enchantmentTypeEnum, subtypes);
+            nms = (VanillaEnchantmentSlotType) nmsEnchantmentType;
+            nms.setOil(this);
+        }
+
+        @Override
+        public boolean canEnchant(NMSItem item) {
+            return nms.canEnchantOriginal((Item) item) || super.canEnchant(item);
+        }
+    }
+
+    public static class SpecialEnchantmentType extends EnchantmentType {
+        private final OilSpecialEnchantmentSlotType nms;
+
+        protected SpecialEnchantmentType(NMSEnchantmentType nmsEnchantmentType, OilMod mod, EnchantmentTypeEnum enchantmentTypeEnum, RealEnchantmentType... subtypes) {
+            super(nmsEnchantmentType, mod.createKey("enchant_type_" + nmsEnchantmentType.toString().toLowerCase()), enchantmentTypeEnum, subtypes);
+            nms = (OilSpecialEnchantmentSlotType) nmsEnchantmentType;
+            nms.setOil(this);
+        }
+    }
+}
